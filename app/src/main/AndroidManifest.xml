<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.a2022realproject_pmplusapp">
    <!-- 인터넷 권한 선언 -->
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.2022RealProject_PMplusApp"
        android:usesCleartextTraffic="true"
        tools:targetApi="m">

        <activity
            android:name=".MainActivity_Weather"
            android:exported="false" />
        <activity
            android:name=".MainActivity_MshipData"
            android:exported="false" />
        <activity
            android:name=".MainActivity_ShipData"
            android:exported="false" />
        <activity
            android:name=".MainActivity_PM_Main"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity> <!-- 첫 실행 화면으로 만들어준거 위 intent어쩌고가 -->
        <activity
            android:name=".MainActivity_Login"
            android:exported="false" />
        <activity
            android:name=".MainActivity_Membership"
            android:exported="false" />
        <activity
            android:name=".MainActivity_Success"
            android:exported="true" />
    </application>

</manifest>